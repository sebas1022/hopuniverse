   	html += '      <span class="input-group-addon"><img src="language/{{ language['code'] }}/{{ language['code'] }}.png" title="{{ language['name'] }}" /> Link</span><input type="text" name="part_url[' + url_row+ '][url][{{ language['language_id'] }}]" value="" placeholder="{{ entry_url }}" class="form-control" />';
