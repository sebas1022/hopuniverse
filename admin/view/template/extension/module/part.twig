}%}
<div id="content">
   <div class="page-header">
      <div class="container-fluid">
         <div class="pull-right">
		  <button type="submit" form="form-part" id="save_stay" name="save_stay" value="1" class="btn btn-success"><i class="fa fa-check"></i> Save and stay</button>
            <button type="submit" form="form-part" data-toggle="tooltip" title="Save" class="btn btn-primary"><i class="fa fa-save"></i></button>
            <a href="{{ cancel }}" data-toggle="tooltip" title="Cancel" class="btn btn-default"><i class="fa fa-reply"></i></a>
         </div>
         <h1>{{ heading_title1 }}</h1>
         <ul class="breadcrumb">
            {% for breadcrumb in breadcrumbs %}
            <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
            {% endfor %}
         </ul>
      </div>
   </div>
   <div class="container-fluid">
      {% if error_warning %}
      <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
         <button type="button" class="close" data-dismiss="alert">&times;</button>
      </div>
      {% endif %}
      {% if success %}
      <div class="alert alert-success"><i class="fa fa-check-circle"></i> Success
         <button type="button" class="close" data-dismiss="alert">&times;</button>
      </div>
      {% endif %}
      <div class="panel panel-default">
         <div class="panel-body">
            <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-part" class="form-horizontal">
            <div class="panel panel-default">
            <div class="panel-body">
               <div class="tab-content">
                  <div class="col-sm-2">
                     <ul class="nav nav-pills nav-stacked">
                  <li class="active"><a href="#tab_1" data-toggle="tab">Header</a></li>
                  <li><a href="#tab_2" data-toggle="tab">Aditional</a></li>
               </ul>
                </div>
                  <div class="col-sm-10">
                     <div class="tab-content">
                  <div id="tab_1" class="tab-pane active">
                                 <div class="form-group">
                                    <label class="col-sm-2 control-label">Sticky header</label>
                                    <div class="col-sm-10">
                                       <div class="btn-group" data-toggle="buttons">
                                          <label class="btn btn-success   {{ part_sticky_header == 1 ? 'active' : '' }}"><input type="radio"  name="part_sticky_header"  {{ part_sticky_header == 1 ? 'checked="checked"' : '' }}  value="1" autocomplete="off">Yes</label>
                                          <label class="btn btn-success   {{ part_sticky_header == 0 ? 'active' : '' }}"><input type="radio"  name="part_sticky_header"  {{ part_sticky_header == 0 ? 'checked="checked"' : '' }}  value="0" autocomplete="off">No</label>
                                       </div>
                                    </div>
                                 </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Custom links in menu</label>
                                    {% set url_row = 0 %}
                                    <div class="col-sm-10">
                                       <div id="menu-url">
                                          {% for url in part_url %}
                                          <div id="url-row{{ url_row }}" class="form-group">
                                             <div class="col-sm-5">
                                                {% for language in languages %}
                                                <div class="input-group"><span class="input-group-addon"><img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" /> Text</span>
                                                   <input type="text" name="part_url[{{ url_row }}][name][{{ language.language_id }}]" value="{{ part_url[url_row] ? part_url[url_row].name[language.language_id] }}" placeholder="{{ entry_name }}" class="form-control" />
                                                </div>
                                                {% endfor %}
                                             </div>
                                             <div class="col-sm-5">
                                                {% for language in languages %}
                                                <div class="input-group"><span class="input-group-addon"><img src="language/{{ language.code }}/{{ language.code }}.png" title="{{ language.name }}" /> Link</span>
                                                   <input type="text" name="part_url[{{ url_row }}][url][{{ language.language_id }}]" value="{{ part_url[url_row] ? part_url[url_row].url[language.language_id] : '' }}" placeholder="{{ entry_url }}" class="form-control" />
                                                </div>
                                                {% endfor %}
                                             </div>
                                             <div class="col-sm-2">
                                                <div class="text-left"><button type="button" onclick="$('#url-row{{ url_row }}').remove();" class="btn btn-danger"><i class="fa fa-minus-circle"></i></button></div>
                                             </div>
                                             {% if error_part_url.url_row %}
                                             <div class="col-sm-10">
                                                <div class="text-danger">{{ error_empty }}</div>
                                             </div>
                                             {% endif %}
                                          </div>
                                          {% set url_row = url_row + 1 %}
                                          {% endfor %}
                                       </div>
                                       <button type="button" onclick="addUrlMenu();" class="btn btn-primary"><i class="fa fa-plus-circle"></i></button>
                                    </div>
                                 </div>
                                 <div class="form-group">
                                    <label class="col-sm-2 control-label">Topbanner show</label>
                                    <div class="col-sm-10">
                                       <div class="btn-group" data-toggle="buttons">
                                          <label class="btn btn-success   {{ part_topbanner_show == 1 ? 'active' : '' }}"><input type="radio"  name="part_topbanner_show"  {{ part_topbanner_show == 1 ? 'checked="checked"' : '' }}  value="1" autocomplete="off">Yes</label>
                                          <label class="btn btn-success   {{ part_topbanner_show == 0 ? 'active' : '' }}"><input type="radio"  name="part_topbanner_show"  {{ part_topbanner_show == 0 ? 'checked="checked"' : '' }}  value="0" autocomplete="off">No</label>
                                       </div>
                                    </div>
                                 </div>
                                 <div class="form-group">
                                    <label class="col-sm-2 control-label">
                                    <a href="" id="part_topbanner_img" data-toggle="image" class="img-thumbnail"><img src="{{ part_topbanner_img_preview }}" alt="" title="" data-placeholder="{{ placeholder }}" /></a>
                                    <input type="hidden" name="part_topbanner_img" value="{{ part_topbanner_img }}" id="part_topbanner_img-input-image" /></label>              
                                    <div class="col-sm-9">
                                       {% for language in languages %} 
                                       <div class="input-group"><span class="input-group-addon"> <img src="language/{{ language['code'] }}/{{ language['code'] }}.png" title="{{ language['name'] }}" /></span>
                                          <input class="form-control" type="text" name="part_topbanner_link[{{ language['language_id'] }}]" value="{{ part_topbanner_link[language['language_id']] is defined ? part_topbanner_link[language['language_id']] : '' }}" />
                                       </div>
                                       {% endfor %} 
                                    </div>
                                 </div>
                              </div>
 <div id="tab_2" class="tab-pane">
				  <div class="form-group">
                                    <label class="col-sm-2 control-label">Fonts:</label>
                                    <div class="col-sm-6">
                                       <select class="form-control " name="part_font">
                                       {% for slug, item in fonts %}
                                       <option value="{{ slug }}" {% if part_font == slug %}selected="selected"{% endif %}>{{ item }}</option>
                                       {% endfor %}
                                       </select>    
                                    </div>
                                 </div>
						         <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-part_color_main">Main color:</label>
                                    <div class="col-sm-6">
                                       <input type="text" name="part_color_main" value="{{ part_color_main }}" placeholder="main color" id="part_color_main" class="form-control" data-jscolor=""/>
                                    </div>
                                 </div>
								 <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-part_color_main">Main color2:</label>
                                    <div class="col-sm-6">
                                       <input type="text" name="part_color_main2" value="{{ part_color_main2 }}" placeholder="main color" id="part_color_main2" class="form-control" data-jscolor=""/>
                                    </div>
                                 </div>
								  <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-part_color_main">Main color3:</label>
                                    <div class="col-sm-6">
                                       <input type="text" name="part_color_main3" value="{{ part_color_main3 }}" placeholder="main color" id="part_color_main3" class="form-control" data-jscolor=""/>
                                    </div>
                                 </div>
								<div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-part_text_color">Text color:</label>
                                    <div class="col-sm-6">
                                       <input type="text" name="part_text_color" value="{{ part_text_color }}" placeholder="text color" id="part_text_color" class="form-control" data-jscolor="" />
                                    </div>
                                 </div>
                                 <div class="form-group">
                                    <label class="col-sm-2 control-label" for="input-part_text_color2">Text second color:</label>
                                    <div class="col-sm-6">
                                       <input type="text" name="part_text_color2" value="{{ part_text_color2 }}" placeholder="text color" id="part_text_color2" class="form-control" data-jscolor=""/>
                                    </div>
                                 </div>
                                 <div class="form-group">
                                    <label class="col-sm-2 control-label">Custom CSS</label>
                                    <div class="col-sm-10">
                                       <textarea name="part_custom_css" id="input-part" placeholder="css" class="form-control code">{{ part_custom_css is defined ? part_custom_css : '' }}</textarea>
                                    </div>
                                 </div>
                                 <div class="form-group">
                                    <label class="col-sm-2 control-label">Custom JS</label>
                                    <div class="col-sm-10">
                                       <textarea name="part_custom_js" id="input-part2" placeholder="js" class="form-control code">{{ part_custom_js is defined ? part_custom_js : '' }}</textarea>
                                    </div>
                                 </div>
                              </div>
               </div>
			   </div>
			   </div>
			   </div>
			   </div>
            </form>
         </div>
      </div>
   </div>
</div>
{{ footer }}
